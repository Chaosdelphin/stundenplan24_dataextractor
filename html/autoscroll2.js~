function upscroll(){
	$('body,html').animate({scrollTop: 0}, 40);
        //setTimeout("downscroll()",2000);
        downscroll();
	}
		function downscroll(){
		//$('body,html').animate({scrollTop: $("#end").offset().top + ($(window).height() - 100)}, 20000);
		$('html,body').animate({scrollTop: document.body.scrollHeight*8},320000);
		//setTimeout("upscroll()",2000);
		//upscroll();
		
